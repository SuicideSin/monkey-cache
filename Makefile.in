all: monkey-cache.so
include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS
DEFS    = $DEFS
CACHE_OBJECTS = cache.o ht.o cache_file.o pipe_buf.o cache_req.o utils.o cJSON.o

-include $(CACHE_OBJECTS:.o=.d)

monkey-cache.so: $(CACHE_OBJECTS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -o $@ $^ -lc -lm
