all: monkey-cache.so
include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS
DEFS    = $DEFS
CACHE_OBJECTS = cache.o ht/ht.o utils/utils.o cJSON/cJSON.o

-include $(CACHE_OBJECTS:.o=.d)

monkey-cache.so: $(CACHE_OBJECTS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -o $@ $^ -lc -lm
